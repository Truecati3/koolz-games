{
  "version": 2,
  "builds": [
    {
      "src": "pages/index.js",
      "use": "@vercel/next"
    },
    {
      "src": "pages/api/login.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
